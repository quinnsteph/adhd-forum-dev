---
import Layout from '../../components/Layout.astro';
import SectionView from '../../components/SectionView';

export function getStaticPaths() {
  return [
    {params: {sectionId: 'public-discussions'}},
    {params: {sectionId: 'getting-started'}},
    {params: {sectionId: 'members-lounge'}},
    {params: {sectionId: 'support-groups'}},
    {params: {sectionId: 'resources'}}
  ];
}

const { sectionId } = Astro.params;
---

<Layout title={`ADHD Forum - ${sectionId}`}>
  <SectionView client:only="react" sectionId={sectionId} />
</Layout>